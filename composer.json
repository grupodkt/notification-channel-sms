    "require": {
        "php": "^8.0.2",
        "illuminate/notifications": "*",
        "guzzlehttp/guzzle": "^7.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.4",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Sms\\": "src"
